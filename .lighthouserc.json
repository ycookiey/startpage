{
  "ci": {
    "collect": {
      "staticDistDir": ".",
      "numberOfRuns": 3
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.9}],
        "categories:accessibility": ["warn", {"minScore": 0.9}],
        "csp-xss": ["warn", {"minScore": 0}],
        "link-text": ["warn", {"minScore": 0}]
      }
    }
  }
}
